  "type": "craft-plugin",
  "keywords": ["craft", "cms", "asset-rev", "scandir", "finder"],
  "license": "MIT",
  "authors": [
    {
      "name": "You & Me Digital",
      "homepage": "https://youandme.digital"
    }
  ],
  "support": {
    "issues": "https://github.com/youandmedigital/craft-getasset/issues/new",
